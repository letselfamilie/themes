<wpml-config>
	<shortcodes>
		<shortcode>
			<tag>laborator_banner</tag>
			<attributes>
				<attribute>title</attribute>
				<attribute encoding="allow_html_tags">description</attribute>
				<attribute encoding="vc_link">href</attribute>
			</attributes>
		</shortcode>
		<shortcode>
			<tag>laborator_blog_posts</tag>
			<attributes>
				<attribute encoding="vc_link">vc_link</attribute>
			</attributes>
		</shortcode>
		<shortcode>
			<tag>laborator_featuretab</tag>
			<attributes>
				<attribute>title</attribute>
				<attribute encoding="allow_html_tags">description</attribute>
				<attribute encoding="vc_link">href</attribute>
			</attributes>
		</shortcode>
		<shortcode>
			<tag>laborator_heading</tag>
			<attributes>
				<attribute>title</attribute>
				<attribute>sub_title</attribute>
			</attributes>
		</shortcode>
		<shortcode>
			<tag>laborator_image_banner</tag>
			<attributes>
				<attribute>title</attribute>
				<attribute encoding="allow_html_tags">description</attribute>
				<attribute encoding="vc_link">vc_link</attribute>
			</attributes>
		</shortcode>
		<shortcode>
			<tag>laborator_logo_carousel</tag>
			<attributes>
				<attribute>title</attribute>
			</attributes>
		</shortcode>
	</shortcodes>
	<custom-types>
		<custom-type translate="1">product</custom-type>
		<custom-type translate="1">testimonial</custom-type>
		<custom-type translate="1">revslider</custom-type>
		<custom-type translate="1">layerslider</custom-type>
	</custom-types>
	<taxonomies>
        <taxonomy translate="1">product_cat</taxonomy>
    </taxonomies>
	<custom-fields>
		<custom-field action="translate">address</custom-field>
		<custom-field action="translate">address_title</custom-field>
		<custom-field action="translate">contact_form_title</custom-field>
		<custom-field action="translate">custom_page_title</custom-field>
		<custom-field action="translate">success_message</custom-field>
		<custom-field action="translate">post_slider_images</custom-field>
		<custom-field action="translate">opened_panels</custom-field>
		<custom-field action="translate">show_map</custom-field>
		<custom-field action="translate">map_coordinates</custom-field>
		<custom-field action="translate">form_position</custom-field>
		<custom-field action="translate">form_title</custom-field>
		<custom-field action="translate">form_sub_title</custom-field>
		<custom-field action="translate">required_fields</custom-field>
		<custom-field action="translate">submit_button_text</custom-field>
		<custom-field action="translate">success_message</custom-field>
		<custom-field action="translate">email_notifications</custom-field>
		<custom-field action="translate">address_title</custom-field>
		<custom-field action="translate">address_sub_title</custom-field>
		<custom-field action="translate">address_description</custom-field>
		<custom-field action="translate">show_page_title</custom-field>
		<custom-field action="translate">main_title</custom-field>
		<custom-field action="translate">small_description</custom-field>
		<custom-field action="translate">show_breadcrumb</custom-field>
		<custom-field action="ignore">product_categories</custom-field>
		<custom-field action="ignore">product_ids</custom-field>
		<custom-field action="ignore">vc_teaser</custom-field>
	</custom-fields>
	<admin-texts>
		<key name="theme_mods_aurum">
			<key name="logo_text" />
			<key name="header_top_left_text" />
			<key name="header_top_right_text" />
			<key name="footer_text" />
			<key name="footer_text_right" />
		</key>
	</admin-texts>
	<language-switcher-settings>
		<key name="icl_language_switcher_sidebar">1</key>
		<key name="icl_widget_title_show">1</key>
		<key name="icl_lang_sel_type">dropdown</key>
		<key name="icl_lso_link_empty">0</key>
		<key name="icl_lso_flags">1</key>
		<key name="icl_lso_native_lang">1</key>
		<key name="icl_lso_display_lang">0</key>
		<key name="icl_lang_sel_footer">0</key>
		<key name="icl_post_availability">1</key>
		<key name="icl_post_availability_position">below</key>
		<key name="icl_post_availability_text">This post is also available in: %s</key>
	</language-switcher-settings>
</wpml-config>